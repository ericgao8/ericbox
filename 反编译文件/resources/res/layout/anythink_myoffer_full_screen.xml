    <include android:id="@+id/anythink_myoffer_include_invalid_button_full_screen" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginBottom="0dp" android:layout_alignParentBottom="true" layout="@layout/anythink_myoffer_include_invalid_button"/>
