    <include layout="@layout/anythink_myoffer_include_guide2click_container"/>
    <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:paddingLeft="12dp" android:paddingTop="12dp" android:paddingRight="12dp" android:paddingBottom="12dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_alignParentRight="true">
        <include android:visibility="gone" android:layout_width="wrap_content" android:layout_height="24dp" layout="@layout/anythink_myoffer_feedback_button"/>
        <com.anythink.basead.ui.CountDownCloseView android:id="@+id/anythink_myoffer_btn_countdown_close_id" android:background="@drawable/anythink_myoffer_base_close_icon" android:visibility="gone" android:layout_width="@dimen/anythink_myoffer_half_screen_button_size" android:layout_height="@dimen/anythink_myoffer_half_screen_button_size" android:layout_marginLeft="12dp"/>
