    <include layout="@layout/anythink_myoffer_include_guide2click_mask"/>
    <com.anythink.basead.ui.PanelView android:id="@+id/anythink_myoffer_banner_view_for_anim_player_id" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_alignParentRight="true" android:layout_alignParentBottom="true"/>
