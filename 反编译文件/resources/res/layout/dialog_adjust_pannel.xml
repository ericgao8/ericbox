<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:background="@drawable/spanel_dialog_bg_white_adjust" android:padding="15dp" android:layout_width="match_parent" android:layout_height="wrap_content">
    <ImageView android:id="@+id/iv_close" android:padding="5dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@mipmap/ic_close_dialog" app:layout_constraintEnd_toEndOf="0" app:layout_constraintTop_toTopOf="0"/>
    <TextView android:textColor="#333333" android:id="@+id/tv_title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="地图设置" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toTopOf="0"/>
    <View android:id="@+id/view_line" android:background="#a7a7a7" android:layout_width="match_parent" android:layout_height="0.5dp" android:layout_marginTop="12dp" app:layout_constraintTop_toBottomOf="@+id/tv_title"/>
    <TextView android:textColor="#3a5bff" android:gravity="center" android:id="@+id/tv_jwd" android:background="@drawable/corner_bg_jwd" android:padding="8dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="12dp" android:text="经纬度" android:paddingStart="36dp" android:paddingEnd="36dp" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintRight_toRightOf="0" app:layout_constraintTop_toBottomOf="@+id/view_line"/>
    <LinearLayout android:orientation="horizontal" android:id="@+id/ll_switch" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_marginTop="10dp" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toBottomOf="@+id/tv_jwd">
        <TextView android:textColor="#000000" android:id="@+id/tv_d_switch" android:layout_width="0dp" android:layout_height="wrap_content" android:text="平面" android:layout_weight="1"/>
        <androidx.appcompat.widget.SwitchCompat android:id="@+id/d_switch" android:layout_width="wrap_content" android:layout_height="24dp" android:textOn="" android:textOff="" app:showText="true"/>
    </LinearLayout>
    <TextView android:textColor="#000000" android:id="@+id/tv_light" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="20dp" android:text="高亮" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toBottomOf="@+id/ll_switch"/>
    <SeekBar android:id="@+id/seek_light" android:layout_width="0dp" android:layout_height="wrap_content" android:maxHeight="15dp" android:max="100" android:progressDrawable="@drawable/seekbar_progress_color" android:minHeight="15dp" android:thumb="@drawable/seek_bar_thumb" android:layout_marginEnd="35dp" android:splitTrack="false" app:layout_constraintBottom_toBottomOf="@+id/tv_light" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toEndOf="@+id/tv_compass" app:layout_constraintTop_toTopOf="@+id/tv_light"/>
    <TextView android:textColor="#000000" android:gravity="right" android:id="@+id/tv_light_seek" android:layout_width="35dp" android:layout_height="wrap_content" android:text="0.12" android:maxLength="4" app:layout_constraintEnd_toEndOf="0" app:layout_constraintTop_toTopOf="@+id/tv_light"/>
    <TextView android:textColor="#000000" android:id="@+id/tv_compass" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="20dp" android:text="对比度" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toBottomOf="@+id/tv_light"/>
    <SeekBar android:id="@+id/seek_compass" android:layout_width="0dp" android:layout_height="wrap_content" android:maxHeight="15dp" android:max="100" android:progressDrawable="@drawable/seekbar_progress_color" android:minHeight="15dp" android:thumb="@drawable/seek_bar_thumb" android:layout_marginEnd="35dp" android:splitTrack="false" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toEndOf="@+id/tv_compass" app:layout_constraintTop_toTopOf="@+id/tv_compass"/>
    <TextView android:textColor="#000000" android:gravity="right" android:id="@+id/tv_compass_seek" android:layout_width="35dp" android:layout_height="wrap_content" android:text="100" android:maxLength="4" app:layout_constraintEnd_toEndOf="0" app:layout_constraintTop_toTopOf="@+id/tv_compass"/>
    <TextView android:textColor="#000000" android:id="@+id/tv_bhd" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="20dp" android:text="饱和度" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toBottomOf="@+id/tv_compass"/>
    <SeekBar android:id="@+id/seek_bhd" android:layout_width="0dp" android:layout_height="wrap_content" android:maxHeight="15dp" android:max="100" android:progressDrawable="@drawable/seekbar_progress_color" android:minHeight="15dp" android:thumb="@drawable/seek_bar_thumb" android:layout_marginEnd="35dp" android:splitTrack="false" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toEndOf="@+id/tv_bhd" app:layout_constraintTop_toTopOf="@+id/tv_bhd"/>
    <TextView android:textColor="#000000" android:gravity="right" android:id="@+id/tv_bhd_seek" android:layout_width="35dp" android:layout_height="wrap_content" android:text="100" android:maxLength="4" app:layout_constraintEnd_toEndOf="0" app:layout_constraintTop_toTopOf="@+id/tv_bhd"/>
    <LinearLayout android:orientation="horizontal" android:id="@+id/ll_one_btn" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="15dp" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toBottomOf="@+id/tv_bhd">
        <Button android:textSize="14dp" android:textColor="#3a5bff" android:id="@+id/btn_restore" android:background="@drawable/corner_bg_jwd" android:layout_width="wrap_content" android:layout_height="36dp" android:text="恢复默认"/>
        <Button android:textSize="14dp" android:textColor="#ffffff" android:id="@+id/btn_ok" android:background="@drawable/spanel_dialog_gongju_white" android:layout_width="wrap_content" android:layout_height="36dp" android:text="确定" android:layout_marginStart="30dp"/>
    </LinearLayout>
    <TextView android:textColor="#000000" android:id="@+id/tv_kz" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="20dp" android:text="地形夸张度" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toBottomOf="@+id/ll_one_btn"/>
    <SeekBar android:id="@+id/seek_kz" android:layout_width="0dp" android:layout_height="wrap_content" android:maxHeight="15dp" android:max="100" android:progressDrawable="@drawable/seekbar_progress_color" android:minHeight="15dp" android:thumb="@drawable/seek_bar_thumb" android:layout_marginEnd="35dp" android:splitTrack="false" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toEndOf="@+id/tv_kz" app:layout_constraintTop_toTopOf="@+id/tv_kz"/>
    <TextView android:textColor="#000000" android:gravity="right" android:id="@+id/tv_kz_seek" android:layout_width="35dp" android:layout_height="wrap_content" android:text="100" android:maxLength="4" app:layout_constraintEnd_toEndOf="0" app:layout_constraintTop_toTopOf="@+id/tv_kz"/>
    <LinearLayout android:orientation="horizontal" android:id="@+id/ll_btn" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="15dp" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toBottomOf="@+id/tv_kz">
        <Button android:textSize="14dp" android:textColor="#3a5bff" android:id="@+id/btn_kz_restore" android:background="@drawable/corner_bg_jwd" android:layout_width="wrap_content" android:layout_height="36dp" android:text="恢复默认"/>
        <Button android:textSize="14dp" android:textColor="#ffffff" android:id="@+id/btn_kz_ok" android:background="@drawable/spanel_dialog_gongju_white" android:layout_width="wrap_content" android:layout_height="36dp" android:text="确定" android:layout_marginStart="30dp"/>
    </LinearLayout>
    <View android:id="@+id/bottom_line" android:background="#a7a7a7" android:layout_width="match_parent" android:layout_height="0.5dp" android:layout_marginTop="14dp" app:layout_constraintTop_toBottomOf="@+id/ll_btn"/>
    <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="15dp" app:layout_constraintTop_toBottomOf="@+id/bottom_line">
        <TextView android:textSize="14dp" android:textColor="#000000" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="飞行动画速度"/>
        <TextView android:textSize="10dp" android:textColor="#666666" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="3dp" android:text="影响飞往搜索结果及地球上其他地点的速度"/>
        <LinearLayout android:background="@drawable/bg_white_speed" android:layout_width="match_parent" android:layout_height="20dp" android:layout_marginTop="11dp">
            <TextView android:textSize="10dp" android:textColor="#666666" android:gravity="center" android:id="@+id/tv_speed_1" android:background="@drawable/bg_speed_select" android:layout_width="0dp" android:layout_height="match_parent" android:text="最慢" android:layout_weight="1"/>
            <TextView android:textSize="10dp" android:textColor="#666666" android:gravity="center" android:id="@+id/tv_speed_2" android:background="@drawable/bg_speed_select" android:layout_width="0dp" android:layout_height="match_parent" android:text="较慢" android:layout_weight="1"/>
            <TextView android:textSize="10dp" android:textColor="#666666" android:gravity="center" android:id="@+id/tv_speed_3" android:background="@drawable/bg_speed_select" android:layout_width="0dp" android:layout_height="match_parent" android:text="默认" android:layout_weight="1"/>
            <TextView android:textSize="10dp" android:textColor="#666666" android:gravity="center" android:id="@+id/tv_speed_4" android:background="@drawable/bg_speed_select" android:layout_width="0dp" android:layout_height="match_parent" android:text="较快" android:layout_weight="1"/>
            <TextView android:textSize="10dp" android:textColor="#666666" android:gravity="center" android:id="@+id/tv_speed_5" android:background="@drawable/bg_speed_select" android:layout_width="0dp" android:layout_height="match_parent" android:text="最快" android:layout_weight="1"/>
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
